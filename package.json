{
  "name": "cypressautomationpractice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "if exist cypress\\reports rmdir /s/q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
  "author": "sathish",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@bahmutov/cy-api": "^2.2.6",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@cypress/xpath": "^2.0.3",
    "@faker-js/faker": "^8.3.1",
    "@shelex/cypress-allure-plugin": "^2.40.1",
    "allure-commandline": "^2.25.0",
    "cy-verify-downloads": "^0.2.3",
    "cypress": "^13.6.3",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.8.1",
    "mocha-allure-reporter": "^1.4.0",
    "start-server-and-test": "^2.0.3"
  },
  "dependencies": {
    "convert-excel-to-json": "^1.7.0",
    "cypress-downloadfile": "^1.2.3",
    "cypress-drag-drop": "^1.1.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-parallel": "^0.14.0",
    "fs-": "^1.0.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "neat-csv": "5.1.0",
    "xlsx": "^0.18.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
